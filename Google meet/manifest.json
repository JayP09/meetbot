{
    "manifest_version": 2,
    "name": "Google Meet",
    "version": "0.001",
    "web_accessible_resources": [
        "assets/*.jpg"
    ],
    "content_scripts": [{
        "matches": [
            "https://meet.google.com/*"
        ],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "html"
    }
}
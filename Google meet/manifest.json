{
    "manifest_version": 2,
    "name": "Google Meet",
    "version": "0.001",
    "content_scripts": [{
        "matches": [
            "https://meet.google.com/*"
        ],
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "html"
        
    }
}